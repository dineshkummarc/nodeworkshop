<!DOCTYPE>
<html>
	<head>
		<title>Socket</title>
		<script src="/socket.io/socket.io.js"></script>
		<script>
		  var socket = io.connect('http://127.0.0.1:3000');
		  socket.on('news', function (data) {
		    console.log(data);
		    socket.emit('my other event', { my: 'data' });
		  });
		</script>
	</head>
	<body>
		<h1>Socket Thing</h1>
	</body>
</html>